<h1>Login:</h1>
<%= form_for :session, url: sessions_path do |f| %>
  <%= render "partials/login_errors", object: @errors %>
  <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Submit", value: "Login" %>
  </p>

<% end %>